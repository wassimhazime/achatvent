{# empty Twig template #}




{% block TableDataBase %}
    {% import "macro/item.twig" as item %}




    {% for titre,tableDataBas in _menu   %}


        <li>
          <a href="#"><i class="fa fa-bar-chart-o fa-fw"></i>  {{ titre }}<span class="fa arrow"></span></a>
          <ul class="nav nav-second-level">
            {% for group in tableDataBas%}

                {% for label, url in group %}

                    {{ item.li_a(url,label) }}

                {% endfor %}


            {% endfor %}
          </ul>
        </li> 


    {% endfor %}



{% endblock %}